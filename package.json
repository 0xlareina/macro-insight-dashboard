  "author": "CryptoSense Team",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "antd": "^5.12.8",
    "axios": "^1.11.0",
    "echarts": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
