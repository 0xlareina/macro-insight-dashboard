{
  "name": "cryptosense-dashboard",
  "version": "1.0.0",
  "description": "Professional Crypto Monitoring Dashboard - Core Edition (BTC, ETH, SOL)",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
